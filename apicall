CREATE OR REPLACE TABLE
  `myproject.mydataset.mytable_deduped` AS
SELECT
  *
FROM (
  SELECT
    *,
    ROW_NUMBER() OVER (
      PARTITION BY cist_id, acct_num, mtn, status_ind
      ORDER BY cist_id  -- no real order needed; this just forces a deterministic tie-breaker
    ) AS rn
  FROM
    `myproject.mydataset.mytable`
)
WHERE
  rn = 1;
