            .withFormatFunction(ac -> new TableRow()
                .set("empName", ac.getEmpName())
                .set("address", new TableRow()
                    .set("street", ac.getAddress().getStreet())
                    .set("city", ac.getAddress().getCity())
                    .set("zipcode", ac.getAddress().getZipcode()))
                .set("phoneNumber", ac.getPhoneNumber())
            )
