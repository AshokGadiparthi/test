gcloud dataflow jobs run spanner-to-bq-job-$(date +%s) \
  --gcs-location gs://dataflow-templates/latest/Cloud_Spanner_to_BigQuery \
  --region us-central1 \
  --parameters \
spannerInstanceId=YOUR_SPANNER_INSTANCE, \
spannerDatabaseId=YOUR_DATABASE_NAME, \
spannerTable=YOUR_TABLE_NAME, \
bigQueryDataset=YOUR_BQ_DATASET, \
bigQueryTable=YOUR_BQ_TABLE, \
bigQueryProject=YOUR_PROJECT_ID, \
createDisposition=CREATE_IF_NEEDED, \
writeDisposition=WRITE_TRUNCATE
