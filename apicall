gcloud spanner databases export sql \
  gs://YOUR_BUCKET/spanner-export/ \
  --instance=YOUR_SPANNER_INSTANCE \
  --database=YOUR_DATABASE_NAME \
  --file-format=CSV \
  --async


bq load \
  --autodetect \
  --source_format=CSV \
  YOUR_DATASET.YOUR_TABLE \
  gs://YOUR_BUCKET/spanner-export/YOUR_TABLE_NAME*.csv
