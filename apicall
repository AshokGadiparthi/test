@FinishBundle
public void finishBundle() {
    // Scan Redis for keys matching namespace
    int namespaceCount = 0;
    String cursor = "0";

    do {
        // Scan keys with pattern sectables:*
        ScanResult<String> scanResult = jedis.scan(cursor, new ScanParams().match(NAMESPACE + "*").count(1000));
        namespaceCount += scanResult.getResult().size();
        cursor = scanResult.getCursor();
    } while (!cursor.equals("0"));

    // Fetch 5 sample records from Redis
    List<String> sampleValues = jedis.scan("0", new ScanParams().match(NAMESPACE + "*").count(5)).getResult();

    // Log Redis Namespace Statistics
    System.out.println("ðŸ“Œ Total Records in Namespace (" + NAMESPACE + "): " + namespaceCount);
    System.out.println("ðŸ“Œ Sample Records from Redis: " + sampleValues);
}
