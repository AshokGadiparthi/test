
                // Handle TIMESTAMP field
                if (spannerRow.getColumnType("acct_estb_ts").equals(Type.timestamp())) {
                    Instant spannerInstant = Instant.parse(spannerRow.getTimestamp("acct_estb_ts").toString());
                    java.time.Instant javaInstant = java.time.Instant.ofEpochMilli(spannerInstant.getMillis());
                    LocalDateTime localDateTime = LocalDateTime.ofInstant(javaInstant, ZoneOffset.UTC);
                    row.set("acct_estb_ts", localDateTime.format(DateTimeFormatter.ISO_LOCAL_DATE_TIME));
                }
