WITH normalized AS (
SELECT
CustID,
AcctNum,
CustLineSeqid,

-- Normalize CJCM
(
SELECT STRING_AGG(x, ', ' ORDER BY x)
FROM UNNEST(
REGEXP_EXTRACT_ALL(
TO_JSON_STRING(CJCM_Value),
r'"([^"]+)"'
)
) AS x
) AS CJCM_Value_Ordered,

-- Normalize SPANNER
(
SELECT STRING_AGG(x, ', ' ORDER BY x)
FROM UNNEST(
REGEXP_EXTRACT_ALL(
TO_JSON_STRING(Spanner_Value),
r'"([^"]+)"'
)
) AS x
) AS Spanner_Value_Ordered

FROM your_table
)

SELECT *
FROM normalized;
