// Example handling of a DATE or TIMESTAMP field
                if (spannerRow.getColumnType("acct_estb_dt").equals(Type.date())) {
                    LocalDateTime localDateTime = spannerRow.getDate("acct_estb_dt").toLocalDateTime();
                    row.set("acct_estb_dt", localDateTime.format(DateTimeFormatter.ofPattern("yyyy-MM-dd HH:mm:ss")));
                } else if (spannerRow.getColumnType("acct_estb_ts").equals(Type.timestamp())) {
                    Instant spannerInstant = new Instant(spannerRow.getTimestamp("acct_estb_ts").toString());
                    LocalDateTime localDateTime = LocalDateTime.ofInstant(spannerInstant.toInstant(), ZoneOffset.UTC);
                    row.set("acct_estb_ts", localDateTime.format(DateTimeFormatter.ofPattern("yyyy-MM-dd HH:mm:ss")));
                }
