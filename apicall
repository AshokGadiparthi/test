// Capture and handle errors by printing or logging
            .getFailedInsertsWithErr()
            .apply("LogErrors", ParDo.of(new DoFn<BigQueryInsertError, Void>() {
                @ProcessElement
                public void processElement(ProcessContext c) {
                    BigQueryInsertError insertError = c.element();
                    // Access the error details
                    String errorMessage = insertError.getError().toString();
                    TableRow failedRow = insertError.getRow();
                    // Print or log the error message and failed row
                    System.out.println("Failed row: " + failedRow);
                    System.out.println("Error: " + errorMessage);
                }
            }));
